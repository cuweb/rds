{
  "name": "@carletonuniversity/rds",
  "private": false,
  "version": "0.7.2",
  "license": "MIT",
  "type": "module",
  "main": "./dist/rds.umd.js",
  "module": "./dist/rds.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist",
    "/dist/index.css"
  ],
  "exports": {
    ".": {
      "import": "./dist/rds.es.js",
      "require": "./dist/rds.umd.js",
      "style": "./dist/index.css",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "concurrently 'yarn dev:css' 'yarn icon-list' 'tsc && vite build --watch'",
    "dev:css": "tailwindcss -w -i ./src/styles/index.css -o ./public/index.css",
    "build": "tsc && yarn format && yarn lint && yarn icon-list && vite build && yarn build:css",
    "build:css": "tailwindcss -m -i ./src/styles/index.css -o ./dist/index.css",
    "storybook": "concurrently 'yarn storybook:css' 'storybook dev -p 6006'",
    "storybook:css": "tailwindcss -w -i ./src/styles/index.css -o ./.storybook/index.css",
    "build-storybook": "concurrently 'yarn build-storybook:css' 'storybook build'",
    "build-storybook:css": "tailwindcss -m -i ./src/styles/index.css -o ./.storybook/index.css",
    "icon-list": "node ./generateIconList.js",
    "packages": "yarn npm-check",
    "format": "prettier -w src",
    "lint": "eslint . --ext .ts,.tsx",
    "prepare": "husky install",
    "clean": "rm -rf node_modules ; yarn install",
    "rebuild": "rm -rf node_modules ; rm yarn.lock ; yarn install"
  },
  "dependencies": {
    "@headlessui/react": "1.7.18",
    "@heroicons/react": "2.1.1",
    "priority-plus": "1.5.1",
    "rds-tailwind-theme": "0.6.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwindcss": "3.4.1"
  },
  "peerDependencies": {
    "@react-google-maps/api": "2.19.3",
    "date-fns": "3.3.1",
    "formik": "2.4.5",
    "react-places-autocomplete": "7.3.0",
    "react-player": "2.14.1",
    "react-quill": "2.0.0",
    "yup": "1.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "18.6.1",
    "@commitlint/config-conventional": "18.6.2",
    "@react-google-maps/api": "2.19.3",
    "@rollup/plugin-terser": "0.4.4",
    "@storybook/addon-a11y": "7.6.17",
    "@storybook/addon-docs": "7.6.17",
    "@storybook/addon-essentials": "7.6.17",
    "@storybook/addon-interactions": "7.6.17",
    "@storybook/addon-links": "7.6.17",
    "@storybook/blocks": "7.6.17",
    "@storybook/react": "7.6.17",
    "@storybook/react-vite": "7.6.17",
    "@storybook/testing-library": "0.2.2",
    "@types/react": "18.2.58",
    "@types/react-dom": "18.2.19",
    "@types/react-places-autocomplete": "7.2.14",
    "@typescript-eslint/eslint-plugin": "7.0.2",
    "@typescript-eslint/parser": "7.0.2",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "10.4.17",
    "concurrently": "8.2.2",
    "date-fns": "3.3.1",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-storybook": "0.8.0",
    "formik": "2.4.5",
    "husky": "9.0.11",
    "npm-check": "6.0.1",
    "postcss": "8.4.35",
    "postcss-import": "16.0.1",
    "prettier": "3.2.5",
    "react-places-autocomplete": "7.3.0",
    "react-player": "2.14.1",
    "react-quill": "2.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "storybook": "7.6.17",
    "typescript": "5.3.3",
    "vite": "5.1.4",
    "vite-plugin-dts": "3.7.3",
    "vite-tsconfig-paths": "4.3.1",
    "yup": "1.3.3"
  }
}
