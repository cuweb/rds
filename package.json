{
  "name": "@carletonuniversity/rds",
  "private": true,
  "version": "0.0.1",
  "license": "MIT",
  "type": "module",
  "main": "./dist/rds-tailwindui.umd.js",
  "module": "./dist/rds-tailwindui.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist",
    "/dist/index.css"
  ],
  "exports": {
    ".": {
      "import": "./dist/rds-tailwindui.es.js",
      "require": "./dist/rds-tailwindui.umd.js"
    }
  },
  "size-limit": [
    {
      "path": "dist/rds-tailwindui.es.js",
      "limit": "100 kB"
    },
    {
      "path": "dist/rds-tailwindui.umd.js",
      "limit": "100 kB"
    }
  ],
  "scripts": {
    "size": "size-limit",
    "format": "prettier -w src",
    "lint": "eslint . --ext .ts,.tsx",
    "dev": "tsc && vite build --watch",
    "build": "tsc && npm run format && npm run lint && vite build && npm run build:css && npm run size",
    "build:css": "tailwindcss -m -i ./src/styles/index.css -o ./dist/index.css",
    "cypress": "concurrently 'npm run cypress:css' 'cypress open'",
    "cypress:ci": "concurrently 'npm run cypress:css' 'cypress run --component'",
    "cypress:css": "tailwindcss -w -i ./src/styles/index.css -o ./cypress/support/index.css",
    "storybook": "concurrently 'npm run storybook:css' 'storybook dev -p 6006'",
    "storybook:css": "tailwindcss -w -i ./src/styles/index.css -o ./.storybook/index.css",
    "build-storybook": "concurrently 'npm run build-storybook:css' 'storybook build'",
    "build-storybook:css": "tailwindcss -m -i ./src/styles/index.css -o ./.storybook/index.css",
    "prepare": "husky install",
    "clean": "rm -rf node_modules ; rm yarn.lock ; yarn install ; yarn build"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@headlessui/react": "^1.7.12",
    "@heroicons/react": "^2.0.16",
    "@size-limit/esbuild": "^8.2.4",
    "@size-limit/esbuild-why": "^8.2.4",
    "@size-limit/file": "^8.2.4",
    "@storybook/addon-a11y": "^7.0.0-beta.58",
    "@storybook/addon-actions": "^7.0.0-beta.58",
    "@storybook/addon-docs": "^7.0.0-beta.58",
    "@storybook/addon-essentials": "^7.0.0-beta.58",
    "@storybook/addon-interactions": "^7.0.0-beta.58",
    "@storybook/addon-links": "^7.0.0-beta.58",
    "@storybook/blocks": "^7.0.0-beta.58",
    "@storybook/react": "^7.0.0-beta.58",
    "@storybook/react-vite": "^7.0.0-beta.58",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.6.0",
    "cypress": "^12.7.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.11",
    "formik": "^2.2.9",
    "husky": "^8.0.3",
    "postcss": "^8.4.21",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.4",
    "prop-types": "^15.8.1",
    "rds-tailwind-theme": "^0.3.0",
    "react-quill": "^2.0.0",
    "size-limit": "^8.2.4",
    "storybook": "^7.0.0-beta.58",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vite-plugin-dts": "^2.0.2",
    "vite-tsconfig-paths": "^4.0.5",
    "yup": "^1.0.2"
  },
  "peerDependencies": {
    "@headlessui/react": "^1.7.12",
    "@heroicons/react": "^2.0.16",
    "formik": "^2.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.2.7",
    "yup": "^1.0.2"
  }
}
