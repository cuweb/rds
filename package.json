    "@commitlint/cli": "19.6.1",
    "@commitlint/config-conventional": "19.6.0",
    "@cypress/react": "9.0.0",
    "@size-limit/preset-big-lib": "11.1.6",
    "@storybook/addon-a11y": "8.5.2",
    "@storybook/addon-docs": "8.5.2",
